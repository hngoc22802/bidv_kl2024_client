{
  "name": "web-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "lint": "tsc --noEmit",
    "fix-eslint": "eslint --ext .tsx,.ts src --fix",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" --ignore-unknown",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@ant-design/pro-components": "^2.6.12",
    "@devexpress/dx-react-core": "^4.0.5",
    "@devexpress/dx-react-scheduler": "^4.0.5",
    "@devexpress/dx-react-scheduler-material-ui": "^4.0.5",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@reduxjs/toolkit": "^1.9.5",
    "antd": "^5.8.2",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-redux": "^8.1.2",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.14.2",
    "sass": "^1.64.2"
  },
  "devDependencies": {
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^20.4.9",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/sortablejs": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.38",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.9"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run fix-eslint",
      "npm run format"
    ]
  }
}
